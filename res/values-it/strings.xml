    <!-- Ongoing action chip -->
    <string name="ongoing_progress_settings_title">Chip azione della barra di stato</string>
    <string name="ongoing_progress_settings_summary">Opzioni della Chip azione della barra di stato</string>
    <string name="ongoing_action_chip_title">Chip azioni in corso</string>
